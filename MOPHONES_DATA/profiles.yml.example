# dbt profile for MoPhones (BigQuery).
# Copy to ~/.dbt/profiles.yml as profiles.yml and set your project/dataset.
# Do not commit profiles.yml if it contains credentials.
mophones_data:
  target: dev
  outputs:
    dev:
      type: bigquery
      method: oauth
      project: your-gcp-project
      dataset: mophones
      threads: 4
    prod:
      type: bigquery
      method: oauth
      project: your-gcp-project
      dataset: mophones
      threads: 4
